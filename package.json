{
  "name": "lamecodashboard",
  "version": "1.0.0",
  "description": "Lameco dashboard system",
  "main": "server.js",
  "author": "Max Altena <Max@MaxAltena.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxAltena/LamecoDashboard.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.4.13",
    "node-sass-chokidar": "^1.3.4",
    "nodemailer": "^4.7.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "^2.1.5",
    "react-swipeable-views": "^0.13.1",
    "react-swipeable-views-utils": "^0.13.1",
    "styled-components": "^4.1.3",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "install-server": "npm install",
    "install-client": "cd client && npm install",
    "install-all": "npm run install-server && npm run install-client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run heroku-build --prefix client"
  },
  "engines": {
    "node": "10.12.0",
    "npm": "6.4.1"
  },
  "homepage": "https://lameco-dashboard.herokuapp.com/"
}
